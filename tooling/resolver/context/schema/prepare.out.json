{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "tooling/resolver/context/prepare output",
  "type": "object",
  "required": [
    "projectPath",
    "cacheRoot",
    "sources",
    "replaceAlias",
    "replaceSpec",
    "warnings"
  ],
  "properties": {
    "projectPath": { "type": "string", "minLength": 1 },
    "cacheRoot": { "type": "string", "minLength": 1 },
    "sources": { "type": "object" },
    "replaceAlias": {
      "type": "object",
      "additionalProperties": { "type": "string" }
    },
    "replaceSpec": { "type": "object" },
    "allowlist": {
      "anyOf": [
        {
          "type": "array",
          "items": { "type": "string", "minLength": 1 }
        },
        { "type": "null" }
      ]
    },
    "warnings": {
      "type": "array",
      "items": { "type": "string" }
    }
  },
  "additionalProperties": false
}
