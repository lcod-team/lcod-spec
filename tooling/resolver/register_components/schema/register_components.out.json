{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "registered": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of components successfully registered."
    },
    "warnings": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Warnings emitted while resolving or registering components."
    },
    "sources": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "url": {
            "type": "string"
          }
        },
        "required": [
          "type"
        ]
      },
      "description": "Map of component IDs to their resolved sources."
    }
  },
  "description": "Register shared resolver helper components via the tooling/resolver/register axiom."
}