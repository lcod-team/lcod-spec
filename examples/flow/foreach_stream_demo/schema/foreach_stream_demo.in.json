{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "foreach_stream_demo input",
  "type": "object",
  "required": ["numbers"],
  "additionalProperties": false,
  "properties": {
    "numbers": {
      "type": "object",
      "description": "Container that holds the stream handle returned by the host runtime.",
      "required": ["stream"],
      "additionalProperties": true,
      "properties": {
        "stream": {
          "type": "object",
          "required": ["id"],
          "additionalProperties": true,
          "properties": {
            "id": { "type": "string" },
            "encoding": { "type": "string" },
            "mediaType": { "type": "string" }
          }
        }
      }
    },
    "attempts": {
      "type": "array",
      "description": "Iteration budget for the foreach loop; length should exceed the expected number of chunks.",
      "items": { "type": "number" }
    }
  }
}
