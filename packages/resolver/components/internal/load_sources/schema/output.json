{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "registrySources": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "id",
          "type"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "priority": {
            "type": "integer"
          },
          "defaults": {
            "type": "object"
          },
          "lines": {
            "type": "array",
            "items": {
              "type": "object"
            }
          },
          "jsonl": {
            "type": "string"
          }
        },
        "additionalProperties": true
      },
      "description": "Inline registry entries ready for indexing."
    },
    "warnings": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Resolved warnings while gathering catalogues."
    },
    "sourcesPath": {
      "type": [
        "string",
        "null"
      ],
      "description": "Path to the sources.json file used for this run."
    }
  },
  "description": "Resolve resolver sources.json into inline registry catalogue entries."
}