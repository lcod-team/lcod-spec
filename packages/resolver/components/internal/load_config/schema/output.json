{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "configPath": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "string",
      "description": "Resolved path that was loaded."
    },
    "config": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "object",
      "description": "Normalised configuration object."
    },
    "warnings": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Warnings produced while loading the configuration."
    },
    "projectPath": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": [
        "string",
        "null"
      ],
      "description": "Passthrough of the project root path."
    },
    "outputPath": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": [
        "string",
        "null"
      ],
      "description": "Passthrough of the optional output path."
    }
  },
  "description": "Load `resolve.config.json`, merge fallbacks, and expose a normalised resolver configuration."
}