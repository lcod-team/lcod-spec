{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["resolverResult", "warnings"],
  "properties": {
    "resolverResult": {
      "type": "object",
      "description": "Resolver result containing root dependency graph and warnings."
    },
    "warnings": {
      "type": "array",
      "items": { "type": "string" },
      "description": "Warnings emitted during resolution."
    },
    "contractSourcesSnapshot": {
      "type": "object",
      "description": "Map of component ids to the source descriptors merged before invoking the contract.",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "type": { "type": "string" },
          "path": { "type": "string" },
          "url": { "type": "string" },
          "reference": { "type": "string" }
        }
      }
    }
  },
  "additionalProperties": true
}
