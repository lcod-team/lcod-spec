{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "resolverResult": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "object",
      "description": "Resolver result containing root dependency graph and warnings."
    },
    "warnings": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Warnings emitted during resolution."
    },
    "contractSourcesSnapshot": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "reference": {
            "type": "string"
          }
        }
      },
      "description": "Snapshot of the sources map passed to the resolver contract."
    }
  },
  "description": "Resolve the dependency graph using normalised configuration and cache information."
}