{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://lcod.team/schemas/contracts/core/fs/write_file/output.json",
  "title": "Filesystem Write File Output",
  "type": "object",
  "additionalProperties": false,
  "required": ["bytesWritten"],
  "properties": {
    "bytesWritten": {
      "type": "integer",
      "minimum": 0,
      "description": "Number of bytes written to disk."
    },
    "mtime": {
      "type": "string",
      "format": "date-time",
      "description": "Last modification timestamp of the file after writing."
    }
  }
}
