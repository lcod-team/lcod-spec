{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://lcod.team/schemas/contracts/core/fs/read_file/output.json",
  "title": "Filesystem Read File Output",
  "type": "object",
  "additionalProperties": false,
  "required": ["data", "size"],
  "properties": {
    "data": {
      "type": "string",
      "description": "File contents encoded with the selected encoding or base64."
    },
    "encoding": {
      "type": "string",
      "enum": ["utf-8", "base64", "hex"],
      "description": "Encoding that was applied to `data`."
    },
    "size": {
      "type": "integer",
      "minimum": 0,
      "description": "Size of the file in bytes."
    },
    "mtime": {
      "type": "string",
      "format": "date-time",
      "description": "Last modification timestamp in ISO 8601 format."
    }
  }
}
