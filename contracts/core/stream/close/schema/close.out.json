{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://lcod.team/schemas/contracts/core/stream/close/output.json",
  "title": "Core Stream Close Output",
  "type": "object",
  "additionalProperties": false,
  "required": ["released"],
  "properties": {
    "released": {
      "type": "boolean",
      "description": "True when the handle was successfully closed."
    },
    "removed": {
      "type": "boolean",
      "description": "True when the backing file was deleted."
    }
  }
}
